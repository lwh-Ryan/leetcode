class Solution:  
    def removeElement(self, A, elem):  
        i = 0  
        for j in range(len(A)):  
            if A[j] != elem:  
                A[i] = A[j]  
                i += 1  
        return i  
        

    def removeElement(self, nums, val):  
            nums[:] = [x for x in nums if x!=val]  
            return len(nums)  
